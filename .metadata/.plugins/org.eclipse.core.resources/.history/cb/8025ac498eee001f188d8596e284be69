package core;

public class P005_Loops {
	public static void main(String[] args) {
		//1.for(initialization;condition;increment/decrement)
		for(int i=6;i<=10;i++) {
			System.out.println(i);
		}
		for(int i=10;i>=1;i--) {
			System.out.println(i);
		}
		
		//2.while(condition)
		int j=11;
		while(j<=10) {
			System.out.println(j);
			j++;
		}
		
		
		//3.do{body} while();
		int k =1;
		do {
			System.out.println(k);
			k++;
		}
		while(k<=10);
	}
}
