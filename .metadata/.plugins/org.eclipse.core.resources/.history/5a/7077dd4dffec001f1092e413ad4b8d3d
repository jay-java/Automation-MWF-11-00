package core;

public class P002_Datatype {
	public static void main(String[] args) {
		//primitive
		byte b = 127;//1
		short s = 3276;//2
		int i = 12;//4
		long l = 12;//8
		float f = 3.143465657657f;//4
		System.out.println(f);
		double d = 3.1435666768;//8
		System.out.println(d);
		char c ='b';
	}
}
