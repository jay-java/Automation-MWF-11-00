package core;

//Arithmetic-> +, -, *, /, %
//assignment-> =, += ,-=, *=, /= 
//unary
//logical
public class P003_Operators {
	public static void main(String[] args) {
		int a = 13, b = 2, c;
		System.out.println(a + b);
		System.out.println(a - b);
		System.out.println(a * b);
		System.out.println(a / b);
		System.out.println(a % b);
		
		a += 25; //a = a + 25
		System.out.println(a);
	}
}
