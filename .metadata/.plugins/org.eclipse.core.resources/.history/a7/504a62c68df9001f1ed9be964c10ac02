package core;
//single
class A {
	public void classA() {
		System.out.println("class A function");
	}
}

class B extends A {
	public void classB() {
		System.out.println("class B function");
	}
}
//multilevel

class C extends B{
	
}
public class P015_Inheritance {
	public static void main(String[] args) {
		B b = new B();
		b.classA();
		b.classB();
	}
}
